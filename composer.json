{
    "name": "lenh/laravel-multichain-api",
    "description": "Laravel 5 wrapper for the Mulitchain php library by Kunstmaan (http://www.kunstmaan.be/)",
    "license": "MIT",
	"keywords": ["laravel", "multichain", "blockchain", "api"],
    "authors": [
        {
            "name": "Leonard Harley",
            "email": "leonard.harley@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^5.3",
        "kunstmaan/libphp-multichain": "^1.0"
    },
	"autoload": {
		"psr-4": {
			"Lenh\\Multichain\\": "src/Lenh/Multichain/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Lenh\\Multichain\\Tests\\": "tests/"
		}
	},
    "require-dev": {
        "phpunit/phpunit": "^5.7",
		"graham-campbell/testbench": "^3.0"
    }
}
